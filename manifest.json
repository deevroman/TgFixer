{
  "manifest_version": 2,
  "name": "TgFixer",
  "version": "1.0.3",
  "description": "__MSG_extensionDescription__",
  "author": "Roman Deev",
  "homepage_url": "https://github.com/deevroman/TgFixer",
  "icons": {
    "48": "icons/border-48.png"
  },
  "permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "src/fixer.js",
        "src/fix_page.js"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "src/popup/popup.html",
    "default_icon": {
      "48": "icons/border-48.png"
    }
  },
  "default_locale": "en"
}